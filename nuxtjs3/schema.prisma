//  prisma使用规范
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite.db"
}

model   tb_demo{
  id            Int     @id @default(autoincrement())
    name      String?  // 昵称
}



